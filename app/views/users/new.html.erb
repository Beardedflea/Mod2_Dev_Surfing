<div class = "row">
<div class = "mx-auto form-group" style="width:30%">
<%= form_for :user, url: '/signup' do |f| %>
      <%= f.label :first_name %>
      <br>
      <%= f.text_field :first_name %>
        <br>
      <%= f.label :last_name %>
        <br>
      <%= f.text_field :last_name %>
        <br>
      <%= f.label :email %>
        <br>
      <%= f.text_field :email %>
        <br>
      <%= f.label :password %>
        <br>
      <%= f.password_field :password %>
        <br>
      <%= f.label :password_confirmation %>
        <br>
      <%= f.password_field :password_confirmation %>
        <br>
      <%= f.submit "Sign Up" %>
<% end %>
</div>
</div>
